{
	"name": "util.html",
	"version": "0.0.11",
	"author": "jmquigley <jmquigley@outlook.com>",
	"description": "HTML helper functions",
	"debug": false,
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmquigley/util.html"
	},
	"bugs": {
		"url": "https://github.com/jmquigley/util.html/issues"
	},
	"engines": {
		"node": ">=7.x"
	},
	"main": "./index.umd.min.js",
	"module": "./index.es.min.js",
	"types": "./index.d.ts",
	"files": [
		"index.*.min.js",
		"index.d.ts"
	],
	"dependencies": {
		"canvas": "^2.5.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "^7.5.5",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-proposal-decorators": "^7.4.4",
		"@babel/plugin-transform-modules-commonjs": "^7.5.0",
		"@babel/plugin-transform-runtime": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/runtime": "^7.5.5",
		"@types/fs-extra": "8.0.0",
		"@types/jest": "^24.0.17",
		"@types/lodash": "^4.14.136",
		"@types/node": "^12.7.1",
		"@types/power-assert": "^1.5.0",
		"@types/puppeteer": "^1.19.1",
		"babel-loader": "^8.0.6",
		"babel-minify": "^0.5.0",
		"babel-plugin-espower": "^3.0.1",
		"babel-preset-minify": "^0.5.0",
		"coveralls": "^3.0.6",
		"debug": "^4.1.1",
		"express": "^4.17.1",
		"fs-extra": "^8.1.0",
		"jest": "^24.8.0",
		"jest-environment-jsdom-fourteen": "^0.1.0",
		"lodash": "^4.17.15",
		"power-assert": "^1.6.1",
		"prettier": "^1.18.2",
		"puppeteer": "^1.19.0",
		"tslint": "^5.18.0",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.5.3",
		"util.calc": "^0.0.20",
		"util.constants": "^0.0.27",
		"util.fixture": "^0.0.72",
		"util.pkgwrap": "^0.0.112",
		"util.toolbox": "^0.0.83",
		"util.wait": "^0.0.37",
		"webpack": "^4.39.1",
		"webpack-cli": "^3.3.6"
	},
	"pkgwrap": {
		"cleanup": [
			"docs",
			"tsconfig.tsbuildinfo"
		]
	},
	"scripts": {
		"all": "yarn install && NODE_ENV=production yarn build && yarn docs && yarn test && yarn manifest",
		"build": "yarn lint && yarn prettier && yarn build:umd && yarn build:es && yarn build:cjs",
		"build:cjs": "babel index.ts --sourceType module --out-file index.cjs.min.js",
		"build:es": "minify index.js --sourceType module --out-file index.es.min.js",
		"build:umd": "pkgwrap --build --webpack",
		"clean": "pkgwrap --clean",
		"clean:dist": "yarn clean && rimraf node_modules/ yarn.lock",
		"coverage": "pkgwrap --coverage --jest",
		"docs": "pkgwrap --docs",
		"lint": "pkgwrap --lint",
		"manifest": "npm pack --dry-run",
		"posttest": "npm stop",
		"pretest": "npm start",
		"prettier": "prettier --write \"**/*.{ts,tsx,js,json,yaml,yml,css}\"",
		"restart": "yarn stop && yarn start",
		"start": "[ ! `pgrep node-http-test` ] && node server.js setting-test &",
		"stop": "pkill --signal SIGINT node-http-test",
		"test": "jest"
	}
}
